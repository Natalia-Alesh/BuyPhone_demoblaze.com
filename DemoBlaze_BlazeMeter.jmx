          <stringProp name="script">
String generateRandomString(int length) { // Генерация случайной строки
    String characters = &apos;ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789&apos;
    StringBuilder result = new StringBuilder()
    for (int i = 0; i &lt; length; i++) {
        int Ind1 = (int) (Math.random() * characters.length())
        result.append(characters.charAt(Ind1))
    }
    return result.toString()
}

String login = &apos;natashka_&apos; + generateRandomString(3) // Генерация логина и пароля
String password = generateRandomString(8)

