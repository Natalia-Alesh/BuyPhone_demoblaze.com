            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">List prices = new ArrayList();
List ids = new ArrayList();
List descriptions = new ArrayList();

for (int i = 1; i &lt;= (vars.get(&quot;price_matchNr&quot;) as int); i++) {
	prices.add(vars.get(&quot;price_&quot; + i) as double)
}
for (int i = 1; i &lt;= (vars.get(&quot;id_matchNr&quot;) as int); i++) {
	ids.add(vars.get(&quot;id_&quot; + i))
}
for (int i = 1; i &lt;= (vars.get(&quot;description_matchNr&quot;) as int); i++) {
	descriptions.add(vars.get(&quot;description_&quot; + i) as String)
}

double maxPrice = Collections.max(prices) as double // Поиск максимальной цены в списке прайсов
String foundDescription = descriptions.find { desc -&gt; // Поиск описания, содержащего текст &quot;16gb&quot; в списке descriptions
  desc.toLowerCase().contains(&quot;16gb&quot;) 
}

int maxPriceIndex = prices.indexOf(maxPrice) // Определение индекса максимальной цены в списке
int foundDescriptionIndex = descriptions.indexOf(foundDescription) // Определение индекса найденного описания в списке
int randomIndex = new Random().nextInt(ids.size()) // Генерация случайного индекса из длинны списка

String maxPricePhoneId = (ids[maxPriceIndex]) // Поиск id телефона с максимальной ценой по индексу
String GB16PhoneId = (ids[foundDescriptionIndex]) // Поиск id телефона с описанием &quot;16GB&quot;
String randomId = (ids[randomIndex]) // Получение случайного id по индексу

vars.put(&quot;maxPricePhoneId&quot;, maxPricePhoneId) // Сохранение id с макс.прайсом в переменную для использования в следующем запросе
vars.put(&quot;GB16PhoneId&quot;, GB16PhoneId) // Сохранение найденного id телефона с 16GB в переменную
vars.put(&quot;randomId&quot;, randomId) // Сохранение случайного id в переменную 

log.info(&quot;Максимальная цена: &quot; + maxPrice) // Логирование
log.info(&quot;Описания всех телефонов: &quot; + descriptions)
log.info(&quot;Найденое описание телефона с 16 GB памяти: &quot; + foundDescription)
log.info(&quot;Id телефона с максимальной ценой: &quot; + maxPricePhoneId)
log.info(&quot;Id телефона с 16 гигами: &quot; + GB16PhoneId)
log.info(&quot;Случайный id: &quot; + randomId)</stringProp>
          </JSR223PostProcessor>
